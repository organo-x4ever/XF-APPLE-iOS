<?xml version="1.0" encoding="UTF-8" ?>

<!-- In a production app/environment, this file should be excluded from being committed to a repo, i.e. add "*/Config/config-sensitive.xml" to your .gitignore file. -->
<!-- The .csproj has a custom PreBuild MSBuild target that copies an empty templated config-sensitive.xml to the ~/Config/ folder before build. -->
<!-- In a production evironment, you could merge your app keys into that templated config-sensitive.xml at build time, thus keeping your sensitive keys out of your repository. -->

<config>
  <!---API Url -->
  <baseUrl>https://mapp.oghq.ca/</baseUrl>
  <!---API Url for Development -->
  <baseUrl_Development>https://mapp.oghq.ca/</baseUrl_Development>
  <!---Authentication Url -->
  <!--<authenticationUrl>Authenticate/authenticate</authenticationUrl>-->
  <authenticationUrl>Authenticate/userauth</authenticationUrl>
  <!-- Token Name -->
  <acceptedTokenName>Token</acceptedTokenName>
  <!-- Access Token Type -->
  <accessTokenType>Basic</accessTokenType>
  <!-- Application Request Header -->
  <applicationRequestHeader>Application</applicationRequestHeader>
  <!-- Welcome Video Url -->
  <welcomeVideoUrl>welcome_video.mp4</welcomeVideoUrl>
</config>